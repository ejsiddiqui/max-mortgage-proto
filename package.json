    "jsdom": "^28.0.0",
    "npm-run-all2": "^5.0.0",
    "postcss": "^8.4.39",
    "prettier": "3.6.2",
    "tailwindcss": "^3.4.4",
    "typescript": "^5.2.2",
    "vite": "^5.3.1",
    "vitest": "^4.0.18"
